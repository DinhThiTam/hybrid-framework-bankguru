<?xml version="1.0" encoding="UTF-8" ?>
<suite name="HRM">
	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/" />
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="reportConfig.MethodListener" />
		<listener class-name="reportConfig.ExtentTestListener" />
		<listener class-name="retryConfig.RetryListener" />
	</listeners>

	<test name="Run on FIREFOX">
		<parameter name="browser" value="firefox" />
		<parameter name="ipAddress" value="192.168.1.8" />
		<parameter name="portNumber" value="6666" />
		<classes>
			<class name="com.hrm.cloud.Level_16_Live_Coding_III_BrowserStack" />
		</classes>
	</test>
	
	
	
